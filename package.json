{
  "name": "cash-pay-server",
  "version": "0.6.3",
  "private": true,
  "scripts": {
    "dev": "npx nodemon ./src/app.js",
    "start": "NODE_ENV=production node ./src/app.js",
    "lint": "eslint ./",
    "format": "eslint ./ --fix",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "docs": "jsdoc src -c jsdoc.json -d ../cash-pay-server-ghpages",
    "docker": "docker build -t developerscash/cash-pay-server:v1-latest . && docker push developerscash/cash-pay-server:v1-latest"
  },
  "dependencies": {
    "@developers.cash/libcash-js": "^0.9.10",
    "axios": "^0.21.1",
    "bitcore-payment-protocol": "^8.1.1",
    "body-parser": "^1.20.1",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "electrum-cash": "^2.0.6",
    "express": "~4.18.2",
    "express-ws": "^4.0.0",
    "jsonschema": "^1.4.0",
    "lodash": "^4.17.21",
    "mongoose": "^5.13.3",
    "socket.io": "^2.4.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^7.15.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "jsdoc": "^3.6.7",
    "nodemon": "^2.0.6",
    "tui-jsdoc-template": "^1.2.2"
  }
}
